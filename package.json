{
  "name": "ShiftPress",
  "version": "1.2.0",
  "description": "WordPress base theme",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tavo1987/shiftpress"
  },
  "keywords": [
    "theme",
    "base",
    "shift",
    "development"
  ],
  "author": "Edwin Ram√≠rez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tavo1987/shiftpress/issues"
  },
  "homepage": "https://github.com/tavo1987/shiftpress#readme",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "foundation-sites": "^6.4.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "cross-env": "^5.0.5",
    "laravel-mix": "^1.4.5"
  }
}
